    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "^2.10.1",
    "components": "^0.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "parcel-bundler": "1.12.4",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sequelize": "^5.21.4",
    "sqlite3": "^4.1.1",
