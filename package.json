  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=dev concurrently \"tsc --pretty --noEmit --watch\" \"tsnd --transpile-only --exit-child ./src/server/index.ts\"",
    "build:server": "parcel build ./src/server/index.ts --target node",
    "build:client": "parcel build ./src/client/index.html --target browser --out-dir=./dist/client",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
